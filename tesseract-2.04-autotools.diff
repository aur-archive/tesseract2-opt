+void reverse32(void *ptr) {
+  char tmp;
+  char *cptr = (char *) ptr;
+
+  tmp = *cptr;
+  *cptr = *(cptr + 3);
+  *(cptr + 3) = tmp;
+  tmp = *(cptr + 1);
+  *(cptr + 1) = *(cptr + 2);
+  *(cptr + 2) = tmp;
+}
+
